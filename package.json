{
  "name": "todo-service",
  "version": "1.0.0",
  "description": "Serverless, Typescript, Todo App",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/pullmana8/todo-service",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "run-s clean && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "describe": "npm-scripts-info",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:eslint": "eslint --ext .ts,.tsx src/",
    "test": "run-s build test:*",
    "test:lint": "eslint build/src/lib/* && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "nyc --silent ava",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ESNext --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ESNext --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash ./build lib test",
    "prepare-release": "run-s reset test cov:check doc:html version doc:publish",
    "webpack": "webpack"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@middy/core": "^1.0.0",
    "@middy/http-cors": "^1.0.0",
    "@middy/http-event-normalizer": "^1.0.0",
    "@middy/http-header-normalizer": "^1.0.0",
    "@middy/http-json-body-parser": "^1.0.0",
    "@sailplane/lambda-utils": "^3.0.0",
    "@sailplane/logger": "^2.0.1",
    "@types/uuid": "^8.0.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.681.0",
    "aws-xray-sdk": "^3.0.1",
    "axios": "^0.19.2",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "middy": "^0.36.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-offline": "^6.1.7",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-reqvalidator-plugin": "^1.0.3",
    "sha.js": "^2.4.11",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@bitjson/typedoc": "^0.15.0-0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/aws-lambda": "^8.10.28",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "ava": "3.8.2",
    "codecov": "^3.7.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.4.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "fork-ts-checker-notifier-webpack-plugin": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "open-cli": "^6.0.1",
    "prettier": "^2.0.5",
    "serverless": "^1.71.3",
    "serverless-jetpack": "^0.10.6",
    "serverless-layers": "^2.2.4",
    "serverless-secret-baker": "^1.2.0",
    "serverless-step-functions": "^2.19.0",
    "standard-version": "^8.0.0",
    "terser-webpack-plugin": "^3.0.1",
    "trash-cli": "^3.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "ava": {
    "failFast": true,
    "files": [
      "dist/lib/**/*.spec.js"
    ],
    "ignoredByWatcher": [
      "dist/lib/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
